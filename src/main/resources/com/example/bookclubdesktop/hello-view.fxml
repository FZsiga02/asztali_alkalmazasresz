<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="600.0" prefWidth="800.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/19" fx:controller="com.example.bookclubdesktop.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Button fx:id="bannButton" mnemonicParsing="false" onAction="#bannClick" text="Tiltás / Tiltás visszavonása" />
      <TableView fx:id="members" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
        <columns>
          <TableColumn fx:id="name" minWidth="100.0" prefWidth="150.0" text="Név" />
          <TableColumn fx:id="gender" minWidth="100.0" prefWidth="150.0" text="Nem" />
            <TableColumn fx:id="birth_date" minWidth="100.0" prefWidth="150.0" text="Születési dátum" />
            <TableColumn fx:id="banned" minWidth="100.0" prefWidth="150.0" text="Kitiltva" />
        </columns>
      </TableView>
   </children>
</VBox>
